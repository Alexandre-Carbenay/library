              "matchers": [
                {
                  "match": "type",
                  "min": 1
                }
              ]
            },
            "$._embedded.editions[*].isbn": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "regex",
                  "regex": "^97[89][0-9]{10}$"
                }
              ]
            },
            "$.description": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.title": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
